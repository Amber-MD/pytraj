    "import pytraj as pt\n",
    "\n",
    "traj = pt.load('traj.nc', top='prmtop')\n",
    "\n",
    "# superpose to first frame, using CA atom\n",
    "traj.superpose(mask='@CA', ref=0)\n",
    "\n",
    "# create trajectory viewer in Jupyter notebook (using nglview)\n",
    "view = traj.view()\n",
    "view"
   ]
